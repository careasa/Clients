<!-- Container to display client entries -->
<div id="clientEntries" class="flex justify-center items-center flex-col mt-6">
    <!-- New client entries will be appended here -->
</div>

<script>
    function enter() {
        // Capture the client name through a prompt
        const clientName = prompt('Enter client name:');
        if (clientName) {
            // Generate a unique ID
            const uniqueID = `J${Math.random().toString(36).substr(2, 9).toUpperCase()}SD${Math.random().toString(10).substr(2, 6)}`;

            // Create a new client entry div
            const clientEntry = document.createElement('div');
            clientEntry.className = "w-[864px] h-20 bg-[#f0f0f0]/0 rounded-lg top-[230px] left-[50px] border-2 border-black overflow-hidden flex items-center justify-center relative mt-4"; // Centering content

            // Add the generated ID inside the div
            clientEntry.innerHTML = `
            <div class="left-[27px] top-[26px] absolute text-black text-2xl font-medium font-['Inter']">#</div>
                <div class="text-black text-2xl top-[26px] left-[50px] font-normal font-['Inter']">${uniqueID}</div>
            `;

            // Append the new client entry to the client entries section
            const clientEntries = document.getElementById('clientEntries');
            clientEntries.appendChild(clientEntry);
        } else {
            alert('Client name cannot be empty.');
        }
    }
</script>

